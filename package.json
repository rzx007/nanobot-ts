{
  "name": "nanobot-ts",
  "version": "0.1.0",
  "description": "Ultra-lightweight personal AI assistant - TypeScript implementation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "nanobot-ts": "dist/cli/run.js"
  },
  "scripts": {
    "build": "bun run bun.config.ts",
    "build:binary": "bun build ./src/cli/run.ts --compile --outfile nanobot",
    "dev": "bun --watch src/cli/run.ts",
    "start": "bun dist/cli/run.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "bun tsc --noEmit",
    "onboard": "bun dist/cli/run.js init",
    "agent": "bun dist/cli/run.js chat --interactive",
    "gateway": "bun dist/cli/run.js gateway",
    "status": "bun dist/cli/run.js status"
  },
  "keywords": [
    "ai",
    "agent",
    "chatbot",
    "llm",
    "typescript",
    "assistant"
  ],
  "author": "Nanobot TypeScript Team",
  "license": "MIT",
  "packageManager": "bun@1.3.10",
  "engines": {
    "bun": ">=1.3.10"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.67",
    "@ai-sdk/deepseek": "^2.0.20",
    "@ai-sdk/google": "^3.0.31",
    "@ai-sdk/groq": "^3.0.24",
    "@ai-sdk/openai": "^2.0.94",
    "@larksuiteoapi/node-sdk": "^1.59.0",
    "@modelcontextprotocol/sdk": "^1.27.1",
    "@openrouter/ai-sdk-provider": "^2.2.3",
    "ai": "^6.0.99",
    "baileys": "7.0.0-rc.9",
    "chalk": "^5.6.2",
    "cheerio": "^1.2.0",
    "commander": "^14.0.3",
    "cosmiconfig": "^9.0.0",
    "croner": "^10.0.1",
    "eventemitter3": "^5.0.4",
    "execa": "^9.6.1",
    "fs-extra": "^11.3.3",
    "iconv-lite": "^0.7.2",
    "imapflow": "^1.2.10",
    "inquirer": "^13.3.0",
    "markdown-it": "^14.1.1",
    "nodemailer": "^8.0.1",
    "ora": "^9.3.0",
    "qrcode-terminal": "^0.12.0",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@types/execa": "^2.0.2",
    "@types/inquirer": "^9.0.9",
    "@types/nodemailer": "^7.0.11",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "bun-types": "latest",
    "eslint": "^10.0.2",
    "json-schema": "^0.4.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "bun": ">=1.3.10"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rzx007/nanobot-ts.git"
  }
}
